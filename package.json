{
  "name": "lerna-monorepo",
  "private": true,
  "version": "0.0.0-development",
  "description": "In-memory MongoDB Server. Designed with testing in mind, the server will allow you to connect your favourite ODM or client library to the MongoDB Server and run integration tests isolated from each other.",
  "repository": "https://github.com/nodkz/mongodb-memory-server",
  "devDependencies": {
    "@types/decompress": "^4.2.3",
    "@types/dedent": "^0.7.0",
    "@types/find-cache-dir": "^2.0.0",
    "@types/find-package-json": "^1.1.0",
    "@types/get-port": "^4.0.1",
    "@types/getos": "^3.0.0",
    "@types/jest": "^24.0.11",
    "@types/lockfile": "^1.0.1",
    "@types/md5-file": "^4.0.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mongodb": "^3.1.22",
    "@types/node": "^11.11.0",
    "@types/tmp": "0.0.34",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "flowgen": "^1.7.0",
    "jest": "^24.3.1",
    "lerna": "^3.13.1",
    "mongodb": ">=3.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "debug": "^4.1.1",
    "decompress": "^4.2.0",
    "dedent": "^0.7.0",
    "find-cache-dir": "^2.0.0",
    "find-package-json": "^1.2.0",
    "get-port": "^4.2.0",
    "getos": "^3.1.1",
    "https-proxy-agent": "^2.2.1",
    "lockfile": "^1.0.4",
    "md5-file": "^4.0.0",
    "mkdirp": "^0.5.1",
    "tmp": "^0.0.33",
    "uuid": "^3.2.1"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "test": "jest --projects packages/*",
    "watch": "yarn test --watchAll",
    "coverage": "yarn test --coverage",
    "build": "tsc --build tsconfig.build.json",
    "release": "yarn build && lerna publish",
    "postinstall": "yarn build"
  }
}
